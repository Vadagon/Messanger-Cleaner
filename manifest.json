{
   "name": "Messenger Cleaner ",
   "description": "Delete All Messages from Facebook Messenger in one click",
   "short_name": "Messenger Cleaner",
   "version": "7",
   "background": {
      "scripts": [ "libs/jquery.js", "libs/core.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "assets/icons/icon_128.png"
      },
      "default_popup": "popup.html",
      "default_title": "Delete All Facebook messages"
   },
   "icons": {
      "128": "assets/icons/icon_128.png"
   },
   "manifest_version": 2,
   "permissions": ["activeTab"],
   "web_accessible_resources": [ "content/*.js" ]
}
