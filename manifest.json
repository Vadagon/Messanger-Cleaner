{
   "background": {
      "scripts": [ "libs/jquery.js", "libs/core.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "assets/icons/icon_128.png"
      },
      "default_popup": "popup.html",
      "default_title": "Delete All Facebook Messanger messages"
   },
   "description": "Fast clean up your Facebook Messenger",
   "icons": {
      "128": "assets/icons/icon_128.png"
   },
   "manifest_version": 2,
   "name": "Delete All Facebook Messanger messages",
   "permissions": ["activeTab"],
   "short_name": "Messanger Cleaner",
   "version": "2",
   "web_accessible_resources": [ "content/*.js" ]
}
